{
  "name": "ingrid-harvester",
  "version": "0.0.0",
  "description": "",
  "main": "server/server.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only app/index.ts",
    "start-cli": "pm2 start ./app/server-cli.js --no-autorestart",
    "start-dev": "ts-node-dev --ignore-watch mappings.json app/index.ts --profile=mcloud",
    "start-diplanung": "cross-env ELASTIC_PREFIX=diplan- ELASTIC_INDEX=harvester && ts-node-dev --poll --ignore-watch mappings.json app/index.ts --profile=diplanung",
    "start-lvr": "cross-env ELASTIC_PREFIX=lvr- ELASTIC_INDEX=harvester && ts-node-dev --poll --ignore-watch mappings.json app/index.ts --profile=lvr",
    "start-ingrid": "ts-node-dev --ignore-watch mappings.json app/index.ts --profile=ingrid --config_dir=./config/ingrid",
    "start-dev-16-inspect": "ts-node-dev --inspect --poll --ignore-watch mappings.json app/index.ts",
    "start-cli-dev": "ts-node app/server-cli.ts",
    "build-with-tests": "tsc --outDir build",
    "build": "tsc --outDir build -p ./tsconfig.build.json",
    "postbuild": "copyfiles './*.json' 'app/**/*.json' 'app/**/*.sql' 'app/**/*.xml' 'app/**/*.md' build/server",
    "install-production": "NODE_ENV=production npm ci",
    "test": "mocha -r ts-node/register test/**/*.ts  --profile=mcloud",
    "test-jenkins": "mocha -r ts-node/register test/mcloud/*.spec.ts --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_path=report.xml  --profile=mcloud",
    "test-jenkins-diplanung": "mocha -r ts-node/register test/diplanung/*.spec.ts --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_path=report.xml  --profile=diplanung"
  },
  "author": "andre.wallat@wemove.com",
  "license": "EUPL",
  "dependencies": {
