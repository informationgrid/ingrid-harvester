    "mocha-jenkins-reporter": "^0.4.2",
    "nodemon": "^2.0.2",
    "sinon": "^8.0.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "./log4j.json",
      "./config.json",
      "./config-general.json",
      "./def_licenses.rdf"
    ]
  }
}
