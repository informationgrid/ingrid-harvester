{
  "name": "mcloud-importer",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register --transpile-only app/index.ts",
    "start-cli": "pm2 start ./app/server-cli.js --no-autorestart",
    "start-dev": "ts-node-dev --ignore-watch mappings.json app/index.ts --profile=mcloud",
    "start-dev-16": "ts-node-dev --poll --ignore-watch mappings.json app/index.ts --profile=diplanung",
    "start-dev-16-inspect": "ts-node-dev --inspect --poll --ignore-watch mappings.json app/index.ts",
    "start-cli-dev": "ts-node app/server-cli.ts",
    "build": "tsc --outDir build",
    "postbuild": "copyfiles './*.json' 'app/**/*.json' build/server",
    "install-production": "NODE_ENV=production npm ci",
    "test": "mocha -r ts-node/register test/**/*.ts  --profile=mcloud",
    "test-jenkins": "mocha -r ts-node/register test/*.spec.ts --colors --reporter mocha-jenkins-reporter --reporter-options junit_report_path=report.xml  --profile=mcloud"
  },
  "author": "andre.wallat@wemove.com",
  "license": "EUPL",
  "dependencies": {
    "@tsed/common": "^5.36.2",
    "@tsed/core": "^5.36.2",
    "@tsed/di": "^5.36.2",
    "@tsed/socketio": "^5.36.2",
    "@turf/centroid": "^6.5.0",
    "@turf/rewind": "^6.5.0",
    "@xmldom/xmldom": "^0.8.6",
    "async": "^3.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cron": "^1.7.2",
    "deep-equal": "^2.0.5",
    "elasticsearch6": "npm:@elastic/elasticsearch@^6.8.7",
    "elasticsearch7": "npm:@elastic/elasticsearch@^7.17.0",
    "elasticsearch8": "npm:@elastic/elasticsearch@^8.5.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "find-port": "^2.0.1",
    "https-proxy-agent": "^5.0.0",
    "iconv-lite": "^0.6.3",
    "js-sha512": "^0.8.0",
    "lodash": "^4.17.21",
    "log4js": "^6.1.0",
    "markdown": "^0.5.0",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
