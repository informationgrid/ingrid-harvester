    "proj4": "^2.8.0",
    "promise": "^8.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "rxjs": "^6.5.2",
    "socket.io": "^2.5.0",
    "sparql-http-client": "^2.2.2",
    "ts-httpexceptions": "^4.1.0",
    "uuid": "^3.3.2",
    "xml-escape": "^1.1.0",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cron": "^1.7.1",
    "@types/elasticsearch": "^5.0.34",
    "@types/express": "^4.17.17",
    "@types/mocha": "^5.2.7",
    "@types/node": "^18.14.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-local": "^1.0.33",
    "@types/request-promise": "^4.1.45",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
