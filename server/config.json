[
  {
    "importer": "EXCEL",
    "elasticSearchUrl": "http://localhost:9200",
    "index": "excel",
    "indexType": "base",
    "alias": "mcloud",
    "filePath": "./data.xlsx",
    "includeTimestamp": true
  },
  {
    "importer": "CKAN-DB",
    "elasticSearchUrl": "http://localhost:9200",
    "index": "ckan_db",
    "indexType": "base",
    "alias": "mcloud",
    "ckanBaseUrl": "https://data.deutschebahn.com",
    "defaultMcloudSubgroup": "railway",
    "includeTimestamp": true
  }
]
