{
  "elasticSearchUrl": "http://localhost:9200",
  "elasticSearchVersion": "8",
  "elasticSearchUser": "elastic",
  "elasticSearchPassword": "elastic",
  "alias": "harvester",
  "numberOfShards": 1,
  "numberOfReplicas": 0,
  "ogcRecordsApiUrl": "http://localhost:8080/v1",
  "ogcRecordsApiUser": "admin",
  "ogcRecordsApiPassword": "admin",
  "cronOffset": 0,
  "proxy": "",
  "portalUrl": "",
  "urlCheck": {
    "active": false,
    "pattern": "30 04 * * *"
  },
  "indexCheck": {
    "active": false,
    "pattern": "10 04 * * *"
  },
  "mail": {
    "enabled": false,
    "mailServer": {
      "host": "localhost",
      "port": 465,
      "secure": false,
      "auth": {
        "user": "",
        "pass": ""
      }
    },
    "from": "test@example.com",
    "to": "test@example.com",
    "subjectTag": "mcloud"
  },
  "indexBackup": {
    "active": false,
    "indexPattern": "mcloud_harvester_statistic|url_check_history|index_check_history",
    "cronPattern": "30 05 * * *",
    "dir": ""
  },
  "maxDiff": 10
}